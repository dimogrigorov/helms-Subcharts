<!ENTITY % EnumCarFeeType "(CANCEL)">
<!ENTITY % EnumApprovalStatus "( TOBEAPPROVED | PREAPPROVED | APPROVED | REJECTED |REJECTEDNOTCANCELED)" >
<!ENTITY % EnumApprovalSource "( INTERNAL | EXTERNAL | INTANDEXT )" >

<!ENTITY % EnumBookingSource "( ONLINE | OFFLINE )" >

<!--          -->
<!-- Elements -->
<!--          -->

<!-- ObjectExchange
    version - the version of the interface
-->
<!ELEMENT ObjectExchange ( Request | Response ) >
<!ATTLIST ObjectExchange
    version %String;    #REQUIRED
>

<!-- Request
-->
<!ELEMENT Request ( Recipient, BookingDetails ) >
<!-- no attributes -->

<!-- Recipient
-->
<!ELEMENT Recipient ( Identifications?, Action ) >
<!-- no attributes -->

<!-- Identifications
-->
<!ELEMENT Identifications ( Identification+ ) >
<!-- no attributes -->

<!-- Identification
    name  - name of the identification information  - RE: ^([^<>&%\*\#\s]([^<>&%\*\#]*[^<>&%\*\#\s])?)?$ - ML: 40
    value - value of the identification information - RE: ^([^<>&%\*\#\s]([^<>&%\*\#]*[^<>&%\*\#\s])?)?$ - ML: 256
-->
<!ELEMENT Identification EMPTY >
<!ATTLIST Identification
    name    %String;    #REQUIRED
    value   %String;    #REQUIRED
>

<!-- Action
    code - the action to be performed
-->
<!ELEMENT Action EMPTY >
<!ATTLIST Action
    code            %EnumAction;     #REQUIRED
    creationDate    %DateAndTime;    #IMPLIED
>

<!-- BookingDetails
-->
<!ELEMENT BookingDetails ( Profiles, Trip, Alternatives? ) >
<!-- no attributes -->

<!-- Profiles
-->
<!ELEMENT Profiles ( Profile+ ) >
<!-- no attributes -->

<!-- Profile
    id             - the id for this profile                           - RE: ^.{1,256}$ - ML: 256
    role           - specifies whether the profile contains booker or traveler information
    greeting       - the booker's or traveler's greeting
    title          - the booker's or traveler's titles                 - ML: 89
    firstName      - the first name of the booker or traveler          - RE: ^[[:alpha:]][[:alpha:]\-\. \']*[[:alpha:]\.\']$ - ML: 40
    middleName     - the middle name of the booker or traveler         - RE: ^[[:alpha:]][[:alpha:]\-\. \']*[[:alpha:]\.\']$ - ML: 40
    lastName       - the last name of the booker or traveler           - RE: ^[[:alpha:]][[:alpha:]\-\. \']*[[:alpha:]\.\']$ - ML: 40
    telephone      - the telephone number of the booker or traveler    - RE: ^[\+0-9 \(\)\/\-]+$ - ML: 40
    fax            - the fax number of the booker or traveler          - RE: ^[\+0-9 \(\)\/\-]+$ - ML: 40
    mobilePhone    - the mobile phone number of the booker or traveler - RE: ^[\+0-9 \(\)\/\-]+$ - ML: 40
    eMail          - the e-mail address of the booker or traveler      - RE: ^[A-Za-z0-9\_\-\.]+\@([A-Za-z0-9]([A-Za-z0-9\-]*[A-Za-z0-9])?\.)+[A-Za-z]{2,5}$ - ML: 128
    employeeNumber - the employeee number of the booker or traveler    - RE: ^.{1,256}$ - ML: 256
    loginName      - the login of the booker or traveler in cytric v7  - RE: ^.{1,256}$ - ML: 256
    GUID           - the global user identified for this profile       - RE: ^.:\d{1,256}$ - ML: 258
-->
<!ELEMENT Profile ( CorporateOrganizations?, FreeDataFields? ) >
<!ATTLIST Profile
    id              %String;            #REQUIRED
    role            %EnumProfileRole;   #REQUIRED
    greeting        %EnumGreeting;      #IMPLIED
    title           %String;            #IMPLIED
    firstName       %String;            #REQUIRED
    middleName      %String;            #IMPLIED
    lastName        %String;            #REQUIRED
    gender          %EnumGender;        #IMPLIED
    telephone       %String;            #IMPLIED
    fax             %String;            #IMPLIED
    mobilePhone     %String;            #IMPLIED
    eMail           %String;            #REQUIRED
    employeeNumber  %String;            #IMPLIED
    loginName       %String;            #IMPLIED
    nationality     %String;            #IMPLIED
    GUID            %String;            #IMPLIED
>

<!-- CorporateOrganizations
-->
<!ELEMENT CorporateOrganizations ( CorporateOrganization+ ) >
<!-- no attributes -->

<!-- CorporateOrganization
    name  - the name of the organization the booker or traveler belongs to
    value - the value of the organization the booker or traveler belongs to - RE: ^.{1,128}$ - ML: 128
-->
<!ELEMENT CorporateOrganization EMPTY >
<!ATTLIST CorporateOrganization
    name    %EnumCorporateOrganization; #REQUIRED
    value   %String;                    #REQUIRED
    id      %String;                    #IMPLIED
    extRef  %String;                    #IMPLIED
>

<!-- FreeDataFields
-->
<!ELEMENT FreeDataFields ( FreeDataField+ ) >
<!-- no attributes -->

<!-- FreeDataField
    name       - the name of a free data field from the booker's or traveler's profile     - RE: ^.{1,128}$ - ML: 128
    value      - the value of this free data field from the booker's or traveler's profile - RE: ^.{1,256}$ - ML: 256
    costCenter - indicator whether the free data field is a cost center
-->
<!ELEMENT FreeDataField EMPTY >
<!ATTLIST FreeDataField
    name        %String;    #REQUIRED
    value       %String;    #REQUIRED
    costCenter  %Boolean;   #IMPLIED
>

<!-- ProfileReference
    id - the identifier of the referenced profile, see //Profile/@id
-->
<!ELEMENT ProfileReference EMPTY>
<!ATTLIST ProfileReference
    id    %String;    #REQUIRED
>

<!-- Alternatives
    Alternatives to the selected trip, for instance, the unselected Air Options
    will be listed here.
-->
<!ELEMENT Alternatives ( Trip+ ) >
<!-- no attributes -->


<!-- Trip
    cytricTripNumber      - the trip id within cytric v7
    externalTripNumber    - the trip id within the customer's system                 - RE: ^.{1,40}$ - ML: 40
    cytricRequestCode     - cTW Requiest ID
    creationDateAndTime   - the date and time of the trip creation withing cytric v7
    identification        - the name or identification of the trip                   - RE: ^.{1,256}$ - ML: 256
    globalTripId          - the trip id within cytric v7 of coherent bookings
    purpose               - the reason why the trip is made                          - RE: ^[^<>&%\*\#\?]*$ - ML: 40
    startDateAndTime      - the date and optionally the time when the trip begins
    completionDateAndTime - the date and optionally the time when the trip ends
    approvalStatus        - the actual approval status at the XML creation time
    approvalDueDate       - date and time till when the trip has to be approved
                            respectively rejected
    approvalDateAndTime   - the date and time when the trip was approved.
    approverComment       - a comment by the approver, regarding the resolution
                             Max length: 256.
    reasonForThisTrip     - a reason, entered by the user for approval purposes
                             Max length: 256.
    approvalSource        - where the approval was or is to be processed.

    lastUpdatedDateAndTime - the date and time of the last change of the trip,
                             that cytric is aware of.
-->
<!ELEMENT Trip ( TripDetails, ExpenseData? ) >
<!ATTLIST Trip
    cytricTripNumber         %Long;          #REQUIRED
    externalTripNumber       %String;        #IMPLIED
    cytricRequestCode        %Long;          #IMPLIED
    creationDateAndTime      %DateAndTime;   #REQUIRED
    identification           %String;        #IMPLIED
    globalTripId             %Long;          #IMPLIED
    purpose                  %String;        #IMPLIED
    startDateAndTime         %DateTime;      #REQUIRED
    completionDateAndTime    %DateTime;      #IMPLIED
    approvalStatus           %EnumApprovalStatus;    #IMPLIED
    approvalDueDate          %DateAndTime;           #IMPLIED
    approvalDateAndTime      %DateAndTime;           #IMPLIED
    approverComment          %String;                #IMPLIED
    reasonForThisTrip        %String;                #IMPLIED
    approvalSource           %EnumApprovalSource;    #IMPLIED
    lastUpdatedDateAndTime   %DateAndTime;           #IMPLIED
    approvalRejectReasonCode %String;                #IMPLIED
>

