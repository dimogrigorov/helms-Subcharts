    cytricRequestDetailNumber   %Long;                  #IMPLIED
    type                        %EnumTripDetailType;    #REQUIRED
    canceled                    %Boolean;               #IMPLIED
    bookingDateAndTime          %DateAndTime;           #REQUIRED
    startDateAndTime            %DateTime;              #REQUIRED
    completionDateAndTime       %DateTime;              #IMPLIED
    duration                    %String;                #IMPLIED
    origin                      %String;                #REQUIRED
    destination                 %String;                #REQUIRED
    originIATACode              %String;                #IMPLIED
    originIATAName              %String;                #IMPLIED
    destinationIATACode         %String;                #IMPLIED
    destinationIATAName         %String;                #IMPLIED
    originCountryCode           %String;                #IMPLIED
    originCountry               %String;                #IMPLIED
    destinationCountryCode      %String;                #IMPLIED
    destinationCountry          %String;                #IMPLIED
    originArea                  %EnumArea;              #IMPLIED
    destinationArea             %EnumArea;              #IMPLIED
    inventorySystem             %EnumInventorySystem;   #IMPLIED
    inventoryRecordLocator      %String;                #IMPLIED
    passiveSystem               %EnumPassiveSystem;     #IMPLIED
    passiveType                 %EnumPassiveType;       #IMPLIED
    passiveRecordLocator        %String;                #IMPLIED
    onHold                      %Boolean;               #IMPLIED
    bookingSource               %EnumBookingSource;     #IMPLIED
    fareLogixLocator            %String;                #IMPLIED
>

