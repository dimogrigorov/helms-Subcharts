<!ELEMENT CarFees ( CarFee+ ) >
<!-- no attributes -->

<!-- CarFee - fees for car
    type         - the car fee type
-->
<!ELEMENT CarFee (Price)>
<!ATTLIST CarFee
    type         %EnumCarFeeType;  #REQUIRED
>

<!-- TripDetailsCloud
    providerName     - the name of the booked appCloud provider
    providerCode     - the cbs code of this provider
    applicationName  - the name of the booked appCloud application.
    applicationCode  - the cbs code of this application
    serviceName      - the name of the booked appCloud service
    rateName         - the name of the booked appCloud rate
    confirmationDate - the date/time when the booked service was confirmed by the provider
-->
<!ELEMENT TripDetailsCloud (Requisite*) >
<!ATTLIST TripDetailsCloud
    providerName      %String;    #REQUIRED
    providerCode      %String;    #IMPLIED
    applicationName   %String;    #REQUIRED
    applicationCode   %String;    #IMPLIED
    serviceName       %String;    #IMPLIED
    rateName          %String;    #IMPLIED
    confirmationDate  %DateTime;  #IMPLIED
>
<!-- Requisite
    name     - the requisite name
    code     - the requisite code
    value    - the value presented as string
-->
<!ELEMENT Requisite EMPTY >
<!ATTLIST Requisite
    name   %String;   #REQUIRED
    code   %String;   #REQUIRED
    value  %String;   #IMPLIED
>

<!-- TravelPolicyAlert
    reasonCode        - the user's explanation why travel policy has been violated, in terms of assigned reason code - ML: 64
    explanation       - the user's free-form explanation why travel policy has been violated - ML: 1024
    reasonCodeComment - Reason code free-text explanation - ML:1024
-->
<!ELEMENT TravelPolicyAlert EMPTY >
<!ATTLIST TravelPolicyAlert
    reasonCode         %String;    #IMPLIED
    explanation        %String;    #IMPLIED
    reasonCodeComment  %String;    #IMPLIED
    advanceReasonCode  %String;    #IMPLIED
    noHotelReasonCode  %String;    #IMPLIED
    noHotelExplanation %String;    #IMPLIED
>

<!-- Response
-->
<!ELEMENT Response ( Error | Confirmation ) >
<!-- no attributes -->

<!-- Error
    code        - the error code
    description - the description of the error
-->
<!ELEMENT Error EMPTY >
<!ATTLIST Error
    code        %Integer;   #IMPLIED
    description %String;    #IMPLIED
>

<!-- Confirmation
    externalTripNumber - the trip id within the customer's system - RE: ^.{1,40}$ - ML: 40
-->
<!ELEMENT Confirmation EMPTY >
<!ATTLIST Confirmation
    externalTripNumber  %String;    #IMPLIED
>

<!-- GeoLocation
     longitude - geographical latitude
     latitude  - geographical longitude
-->
<!ELEMENT GeoLocation EMPTY >
<!ATTLIST GeoLocation
    name      %String;   #IMPLIED
    longitude %Double;   #REQUIRED
    latitude  %Double;   #REQUIRED
>
<!-- CarbonEmission
    amount - ammount of emissions
    unit   - unit of measurement
-->
<!ELEMENT CarbonEmission EMPTY >
<!ATTLIST CarbonEmission
    amount   %Float;    #REQUIRED
    unit     %String;   #REQUIRED
>
